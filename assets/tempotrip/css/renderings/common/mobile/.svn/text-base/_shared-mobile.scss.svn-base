@import "../../../globals/variables";
@import "../../../globals/mixins";
@include max-screen(xl) {
    
}
@include max-screen(large) {
	.container-fluid{
		
	}
	.title-container{
	    width:100%;
	    height:auto;
	    float:left;
	    & h1{
	    	font-size:36px;
	    }
	}
	& .seperator-dots{
		margin-bottom:20px;
	}
	& .introductory-text{
		padding-top:0px;
		& p{
			font-size:26px;
		}
	}
	/* Breadcrumb*/
	#breadcrumbs{
		margin-top:165px;
		& .row{
			margin:0px auto;
			& .col-sm-12{
				padding-left:0px;
				padding-right:0px;
				ol.breadcrumb{
					background-color:transparent;
					padding:0px 0px;
					margin-bottom:40px;
					& li.breadcrumb-item{
						font-family: $Marvel-Regular;
						font-size:16px;
						letter-spacing:0.2px;
						color:$slate-grey;
						& a{
							font-family: $Marvel-Regular;
							font-size:16px;
							letter-spacing:0.2px;
							color:$skyblue;
							&:hover{
								text-decoration:none;
							}	
						}
					}
					
				}
			}
		}
	}
	/*Page Header*/
	.title{
		width:100%;
		margin:0px auto;
		& h1{
			float:left;
		}
		& a.cta{
			float:right;
		}
	}
}
@include max-screen(mediumLarge){
	.container-fluid{
	}
	/*breadcrumbs*/
	#breadcrumbs{
		margin-top:0px;
		& .row{
			margin:0px auto;
			padding-left:40px;
			padding-right:40px;
			& .col-sm-12{
				padding-left:0px;
				padding-right:0px;
				ol.breadcrumb{
					background-color:transparent;
					padding:0px 0px;
					margin-bottom:40px;
					& li.breadcrumb-item{
						font-family: $Marvel-Regular;
						font-size:16px;
						letter-spacing:0.2px;
						color:$slate-grey;
						& a{
							font-family: $Marvel-Regular;
							font-size:16px;
							letter-spacing:0.2px;
							color:$skyblue;
							&:hover{
								text-decoration:none;
							}	
						}
					}
					
				}
			}
		}
	}
	/*introductory text*/
	& .introductory-text{
		padding-left:40px;
		padding-right:40px;
		& p{
			font-size:26px;
		}
	}
	/*Page Header*/
	#page-header{
		& .row{
			padding-left:40px;
			padding-right:40px;
		}
	}
	/*Generic Three Block Listing*/
	#generic-three-block-listing{
		padding-left:40px;
		padding-right:40px;
		margin-bottom:20px;
		padding-bottom:0px;
		& .row{
			padding-left:0px;
			padding-right:0px;
			margin:0px auto 0px auto;
			& .col-sm-4{
				float:left;
				height:auto;
				position:relative;
				width:33.333%;
				& .story-title{
					font-family: $Marvel-Regular;
					font-size: 18px;
					font-weight: 300;
					line-height: 1.67;
					letter-spacing: 2px;
					text-transform:uppercase;
					margin-top:10px;
					margin-bottom:5px;
					color:$skyblue;
					&:hover{
						text-decoration: none;
					}
				}
				& .story-subject{
					font-family:$Marvel-Regular;
					font-size: 26px;
					line-height: 1.25;
					letter-spacing: 0.2px;
					color:$black;
					margin-bottom:20px;
					&:hover{
						text-decoration: none;
					}
				}
				& img{
					width:100%;
					height:auto;
					&:hover, &:focus, &:active{
						text-decoration: none;
						outline-width:0px;
						border:0px;

					}
				}
			}
		}
	}
	/*Search Page*/
	#download-pdf-link{
		& .row{
			margin:0px auto;
			padding-left:40px;
			padding-right:40px;
		}
	}
	#sharing-buttons{
		& .row{
			margin:0px auto;
			padding-left:40px;
			padding-right:40px;
		}
	}
	#search-page{
		margin:0px auto;
		padding-left:0px;
		padding-right:0px;
		padding-bottom:60px;
		& .row{
			padding-right:40px;
			padding-left:40px;
			margin:0px auto;
			& .col-sm-12{
				padding:0px 0px;
			}
		}
		& #search-container{
			max-width:1140px;
			& .col-sm-12{
				& .search-input-container{
					width:100%;
					height:80px;
					padding:20px 20px;
					background-color:$white;
					margin:0 auto 60px auto;
					position:relative;
					border:1px solid $black;
					& .search-input{
						background-color:$white;
						font-family:$Marvel-Regular;
						font-size: 30px;
						font-weight:normal;
						letter-spacing: 0.5px;
						width:85%;
						float:left;
						border:none;
						outline:none;
						border-radius:0px;
					}
					& .search-btn-3{
						width:44px;
						height:30px;
						padding:0px 0px;
						background:url('../images/global/search-hover.svg') right 0px no-repeat;
						background-size:70% auto;
						position:relative;
						right:0px;
						top:5px;
						float:right;
						border:none;
						outline:none;
						cursor:pointer;
					}
				}
				& .row{
					padding-right:0px;
					padding-left:0px;

					& .col-sm-8{
						padding-left:0px;
						width:60%;
						float:left;
						h2{
							width:100%;
							text-align:center;
							margin-bottom:30px;
						}
						& .row{
							& #search-results{
								opacity:0;
								& #searchData{
									margin:0px;
									padding:0px;
									float:left;
									list-style-type: none;
									& li{
										padding-bottom:20px;
										& p.Body-7-1{
											white-space:wrap; 
										    width:100%; 
										    overflow: hidden;
										    text-overflow: ellipsis; 
										}
									}

								}
								#searchData-pagination{
									width:100%;
									margin:0px auto;
									& #searchData-previous{
										float:left;
										font-family: $Marvel-Regular;
										color:$skyblue;
										font-size:16px;
									}
									& #searchData-next{
										float:right;
									}
								}
								& .holder {
									margin: 0 auto;
									width:100%;
									& a {
										float:left;
										font-family: $Marvel-Regular;
										color:$slate-grey;
										font-size:16px;
										margin:0 5px;
										opacity:1;
										&:hover {
											text-decoration: none;
											color:$black;
										}
										&.jp-previous{float:left;color:$skyblue;margin-right:25%;}
										&.jp-next{float:right;color:$skyblue;}
										&.jp-first { margin-right: 0px; float:left; color:$skyblue;
											&:before{
												content:'<';
												font-family: $Marvel-Regular;
												color:$skyblue;
												font-size:12px;
											}
										}
										&.jp-last { margin-left: 0px; float:left; color:$skyblue;
												&:after{
													content:'>';
												font-family: $Marvel-Regular;
												color:$skyblue;
												font-size:14px;
												}
										}
										&.jp-current{ 
											font-weight: bold;
											color:$skyblue;
											opacity:1;
											&:hover { 
												text-decoration:none;
												font-weight: bold;
											}
										}
										&.jp-disabled{
											opacity:0.5;
											color:$slate-grey;
											&.jp-disabled:hover {
												color: #bbb;
											}
										}
										&.arrowPrev, .arrowNext { width:29px; height:29px; top: 55px; cursor: pointer;color:$skyblue;}
										& .arrowPrev { content:'<';  }
										& .arrowNext { content:'>;' }
									}
									& span{margin: 0 5px; float:left;}
								}
								&.on{
									opacity:1;
									transition:all 0.5s 0s linear;
								}
							}
						}
					}
					& .col-sm-4{
						background-color:rgba(0,0,0,0.04);
						padding:0px;
						width:39%;
						float:right;
						& .suggestionsBox{
							padding:20px;
							display:block;
							& .Date{text-transform:uppercase;
							}
							& .seperator-dots{
							    text-align:left;
							    width:100%;
							    margin-top:0px;
							    line-height:0px;
							}
							& ul{
								margin:0px 0px 30px 0px;
								padding:0px;
								list-style-type: none;
								& li{
									padding:5px 0px;
								}
								& a{
									font-family: $Marvel-ExtraLight;
									color:$slate-grey;
									line-height:1.25;
									font-size:16px;
								}
							}
						}
					}
				}
			}
		}
	}
