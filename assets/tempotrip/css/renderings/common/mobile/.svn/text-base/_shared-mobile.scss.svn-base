				& .table{
					margin-bottom:45px;
					& tbody{
						& tr{
							min-height:100px;
							height:auto;
							border-top:1px solid $slate-grey;
							& th, td{
								padding:25px 10px 25px 20px;
								border-top:none;
								margin:0px;
								width:40%;
								vertical-align: middle;
								& p{
									margin:0px 0px;
									font-family:$Marvel-Regular;
									font-size:14px;
									text-align: left;
									line-height:24px;
									color:$slate-grey;
									display:table;
									width:100%;
									float:left;
								}
							}
							& td:first-child{
								width:30%;
							}
							& td:last-child{
								width:30%;
								padding-right:30px;
								
							}
						}
						& tr:first-child{
							min-height:50px;
							height:auto;
							border-top:none;
							background-color: rgba(0,0,0,0.3);
						}
						& tr:nth-child(even){
							background-color: rgba(0,0,0,0.04);
							& td:last-child{
								& .arrow{
									&::before{
										border-left-color:$white;
									}
								}
							}
						}
						& tr:last-child{
							border-bottom:1px solid $slate-grey;
						}
					}
				}
			}
			& .modal{
				display:none;
				background-color:rgba(0,0,0,0.4);
			}
			& .modal-dialog{
				margin-top:30%;
				& .modal-content{
					border-radius:0px;
					border:none;
				}
				& .modal-body{
					& p{
						font-family: $Marvel-Regular;
						font-size:16px;
						color:$slate-grey;
					}
				}
				& .modal-header, & .modal-footer{
					border-top:none;
					border-bottom:none;
					& .btn{
						float:left;
						border-radius:0px;
						border:none;
						text-align:left;
						padding-left:0px;
						&:hover{
							background-color:transparent;
							box-shadow:none;
							color:$skyblue;
						}
						&:focus{
							outline:none;
						}

					}
					& .close{
						width: 50px;
					    height: 50px;
					    background: url(../images/global/white-close-x.svg) 0px 0px no-repeat;
					    background-size: 100% auto;
					    position: absolute;
					    right: 0px;
					    top: -70px;
					    z-index: 6;
					    opacity:1;
					    &:focus{
					    	outline-color: transparent;
					    	outline:none;
					    }
					}
				}
			}
		}
	}
}
@include max-screen(medium) {
	/*Title Component*/
	.title-container{
	    width:100%;
	    height:auto;
	    float:left;
	    & h1{
	        text-align:center;
	        height:auto;
	        & span{
	            display:block;
	        }
	    }
	}
	/* Seperator Dotted line Component*/
	.seperator-dots{
		width:auto;
		margin:0px auto 20px;
		font-family: $Marvel-Regular;
		color:$black;
		font-size: 18px;
	    line-height: 1.15;
	    letter-spacing: 0.3px;
	    text-align:center;
	    width:100%;
	}
	/* Introductory Page Text*/
	.introductory-text{
		width:100%;
		margin:0px auto 30px;
		padding:10px 60px;
		& p{
			font-family: $Marvel-Regular;
			font-size: 24px;
			line-height: 1.67;
			letter-spacing: 0.5px;
			text-align: center;
			color:$slate-grey;
			margin:0px;
			padding:0px;
		}
	}
	/*breadcrumbs*/
	#breadcrumbs{
		margin-top:80px;
		& .row{
			margin:0px auto;
			& .col-sm-12{
				padding-left:0px;
				padding-right:0px;
				ol.breadcrumb{
					background-color:transparent;
					padding:0px 0px;
					margin-bottom:40px;
					& li.breadcrumb-item{
						font-family: $Marvel-Regular;
						font-size:16px;
						letter-spacing:0.2px;
						color:$slate-grey;
						& a{
							font-family: $Marvel-Regular;
							font-size:16px;
							letter-spacing:0.2px;
							color:$skyblue;
							&:hover{
								text-decoration:none;
							}	
						}
					}
					& li.breadcrumb-item.active{
						&::before{
							content:">";
						}
					}
				}
			}
		}
	}
	/*Page Header*/
	.title{
		width:100%;
		margin:0px auto;
		
		& h1{
			float:left;
		}
		& a.cta{
			float:right;
		}
	}
	/*Search Page*/
	#search-page{
		margin:0px auto;
		padding-left:0px;
		padding-right:0px;
		padding-bottom:60px;
		& #search-container{
			& .col-sm-12{
				& .search-input-container{
					width:100%;
					height:80px;
					padding:20px 20px;
					background-color:$white;
					margin:0 auto 60px auto;
					position:relative;
					border:1px solid $black;
					& .search-input{
						background-color:$white;
						font-family:$Marvel-Regular;
						font-size: 30px;
						font-weight:normal;
						letter-spacing: 0.5px;
						width:85%;
						float:left;
						border:none;
						outline:none;
						border-radius:0px;
					}
					& .search-btn-3{
						width:44px;
						height:30px;
						padding:0px 0px;
						background:url('../images/global/search-hover.svg') right 0px no-repeat;
						background-size:70% auto;
						position:relative;
						right:0px;
						top:5px;
						float:right;
						border:none;
						outline:none;
						cursor:pointer;
					}
				}
				& .row{
					padding-right:0px;
					padding-left:0px;

					& .col-sm-8{
						padding-left:0px;
						width:100%;
						float:left;
						h2{
							width:100%;
							text-align:center;
							margin-bottom:30px;
						}
						& .row{
							& #search-results{
								opacity:0;
								& #searchData{
									margin:0px;
									padding:0px;
									float:left;
									list-style-type: none;
									& li{
										padding-bottom:20px;
										& p.Body-7-1{
											white-space:wrap; 
										    width:100%; 
										    overflow: hidden;
										    text-overflow: ellipsis; 
										}
									}

								}
								#searchData-pagination{
									width:100%;
									margin:0px auto;
									& #searchData-previous{
										float:left;
										font-family: $Marvel-Regular;
										color:$skyblue;
										font-size:16px;
									}
									& #searchData-next{
										float:right;
									}
								}
								& .holder {
									margin: 0 auto;
									width:100%;
									& a {
										float:left;
										font-family: $Marvel-Regular;
										color:$slate-grey;
										font-size:16px;
										margin:0 5px;
										opacity:1;
										&:hover {
											text-decoration: none;
											color:$black;
										}
										&.jp-previous{float:left;color:$skyblue;margin-right:14%;}
										&.jp-next{float:right;color:$skyblue;}
										&.jp-first { margin-right: 0px; float:left; color:$skyblue;
											&:before{
												content:'<';
												font-family: $Marvel-Regular;
												color:$skyblue;
												font-size:12px;
											}
										}
										&.jp-last { margin-left: 0px; float:left; color:$skyblue;
												&:after{
													content:'>';
												font-family: $Marvel-Regular;
												color:$skyblue;
												font-size:14px;
												}
										}
										&.jp-current{ 
											font-weight: bold;
											color:$skyblue;
											opacity:1;
											&:hover { 
												text-decoration:none;
												font-weight: bold;
											}
										}
										&.jp-disabled{
											opacity:0.5;
											color:$slate-grey;
											&.jp-disabled:hover {
												color: #bbb;
											}
										}
										&.arrowPrev, .arrowNext { width:29px; height:29px; top: 55px; cursor: pointer;color:$skyblue;}
										& .arrowPrev { content:'<';  }
										& .arrowNext { content:'>;' }
									}
									& span{margin: 0 5px; float:left;}
								}
								&.on{
									opacity:1;
									transition:all 0.5s 0s linear;
								}
							}
						}
					}
					& .col-sm-4{
						display:none;
						
					}
				}
			}
		}
	}
	/*Generic Accordian Listing*/
	#generic-accordian-listing{
		margin-top:0px;
		padding-left:0px;
		padding-right:0px;
		& .row{
			
			& .col-sm-12{
				
				& .panel-group{
					
					& .panel{
						
						& .panel-heading{
							
							& .panel-title{
								& .product-name{
									
									& p{
										margin:0px;
										float:left;
										font-family:$Marvel-Regular;
										font-size:20px;
										line-height:24px;
										letter-spacing:2.1px;
										color:$skyblue;
										text-transform:uppercase;
									}
								}
							}
							& .accordianBtn{
							}
						}
						& .panel-body{
							border-top:0px;
							border-color:transparent;
							padding:0px 40px 40px 40px;
							& .description{
								width:90%;
								& p{
									font-family: $Marvel-Regular;
									font-size:18px;
									letter-spacing:0.3px;
									line-height:30px;
									color:$slate-grey;
								}
							}
						}
						& .panel-collapse{
						}
					}
					& .panel:nth-child(odd){
						background-color: rgba(0,0,0,0.04);
					}
					& .panel:last-child(){
							border-bottom:1px solid $slate-grey;
						}
				}
				& a.cta{
					margin:60px auto;
				}
			}	
		}
	}
	/* generic 3 block listing*/
	#generic-three-block-listing{
		padding-left:40px;
		padding-right:40px;
		& .row{
			padding-left:0px;
			padding-right:0px;
			margin:0px auto 0px auto;
			& .col-sm-4{
				float:left;
				height:auto;
				position:relative;
				width:100%;
				padding-left:0px;
				padding-right:0px;
				& .story-title{
					font-family: $Marvel-Regular;
					font-size: 18px;
					font-weight: 300;
					line-height: 1.67;
					letter-spacing: 2px;
					text-transform:uppercase;
					margin-top:10px;
					margin-bottom:5px;
					color:$skyblue;
					&:hover{
						text-decoration: none;
					}
				}
				& .story-subject{
					font-family:$Marvel-Regular;
					font-size: 32px;
					line-height: 1.25;
					letter-spacing: 0.2px;
					color:$black;
					margin-bottom:20px;
					&:hover{
						text-decoration: none;
					}
				}
				& img{
					width:100%;
					height:auto;
					&:hover, &:focus, &:active{
						text-decoration: none;
						outline-width:0px;
						border:0px;

					}
				}
				&:first-child{
					padding-left:0px;
					padding-right:0px;
				}
				&:last-child{
					padding-left:0px;
					padding-right:0px;
				}
			}
		}
	}
}
